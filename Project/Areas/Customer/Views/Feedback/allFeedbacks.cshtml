@model X.PagedList.PagedList<FeedbackViewModel>

    <div class="container">
        <table class="table table-stripped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Repair Task</th>
                    <th>Content</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var feedback in Model) {
                    <tr>
                        <td>@feedback.Id</td>
                        <td>@feedback.RepairTaskId</td>
                        <td>@feedback.Content</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    @if(Model.Count != 0) {
        @Html.PagedListPager(Model, 
            page => Url.Action(StringConstants.ActionNameEditFeedbackForARepairTask, StringConstants.FeedbackControllerName, 
            new { page }))
    }