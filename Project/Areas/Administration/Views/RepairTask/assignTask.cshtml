@model AvailableTechniciansForRepairTaskViewModel
<form method="post"
      asp-action="@StringConstants.ActionNameAssignTask"
      asp-controller="@StringConstants.RepairTaskControllerName"
      asp-area="@StringConstants.AreaNameAdministration"
      asp-route-id="@Model.TaskId">
    <select asp-for="@Model.SelectedTechnicians" multiple>
        @foreach (var technician in Model.AvailableTechnicinsName) {
            <option value="@technician">@technician</option>
        }
        </select>
        <input type="submit" class="btn btn-primary" />
</form>

